<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CellLayout">
        <attr name="cell_height_count" format="integer">
            <flag name="cell_height_count" value="4"/>
        </attr>
        <attr name="cell_width_count" format="integer">
            <flag name="cell_width_count" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CellView">
        <attr name="cell_width" format="integer">
            <flag name="cell_width" value="4"/>
        </attr>
        <attr name="cell_height" format="integer">
            <flag name="cell_height" value="4"/>
        </attr>
        <attr name="cell_x" format="integer">
            <flag name="cell_x" value="4"/>
        </attr>
        <attr name="cell_y" format="integer">
            <flag name="cell_y" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="Slider">
        <attr name="min" format="integer">
            <flag name="min" value="4"/>
        </attr>
        <attr name="max" format="integer">
            <flag name="max" value="4"/>
        </attr>
        <attr name="value" format="integer">
            <flag name="value" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="circleColor">
            <flag name="circleColor" value="16"/>
            </attr>
        <attr name="circleProgressColor">
            <flag name="circleProgressColor" value="16"/>
        </attr>
        <attr name="circleWidthPercent">
            <flag name="circleWidthPercent" value="2"/>
        </attr>
        <attr name="textColor">
            <flag name="textColor" value="16"/>
        </attr>
        <attr name="maxValue">
            <flag name="maxValue" value="4"/>
        </attr>
        <attr name="progress">
            <flag name="progress" value="32"/>
        </attr>
        <attr name="startAngle">
            <flag name="startAngle" value="4"/>
        </attr>
        <attr name="finishAngle">
            <flag name="finishAngle" value="4"/>
        </attr>
    </declare-styleable>

</resources>